


<div class="content-container">
  <div class="container-fluid">

    <div class="row">
      {{#each users}}
      <div class="col-sm-2 mb-5">
        <div class="card">
          <div class="card-body">
            <img class="img-fluid" src="{{ this.avatar }}"/>
            <h3 class="card-title text-center">{{ this.username }}</h3>
          </div>
        </div>
      </div>
    {{/each}}
    </div>
  </div>
</div>